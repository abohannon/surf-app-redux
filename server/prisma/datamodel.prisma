type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
}

type Buoy {
  id: ID! @id
  date: String
  windDirection: String
  windSpeed: String
  waveHeight: String
  dominantWavePeriod: String
  averageWavePeriod: String
  meanWaveDirection: String
  airTemp: String
  waterTemp: String
  tide: String
}